<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module rename-to="forms">
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.core.Core"/>
    <inherits name="com.google.gwt.user.History"/>
	<inherits name="com.google.gwt.i18n.I18N"/>

	<inherits name="com.smartgwt.SmartGwtNoScript" />
	<inherits name="com.smartclient.SmartClientDefault" />
    <inherits name="com.smartgwt.tools.SmartGwtTools"/>

	<inherits name="sk.qbsw.sgwt.winnetou.Winnetou" />

	<!-- Specify the app entry point class.                         -->
	<entry-point class='sk.qbsw.sgwt.winnetou.ribana.forms.client.Forms' />

	<!-- Specify the application specific style sheet.              -->
	<stylesheet src='forms.css' />
</module>
