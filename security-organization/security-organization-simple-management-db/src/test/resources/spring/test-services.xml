    <bean class="sk.qbsw.security.management.db.autoconfigure.SecurityManagementAutoConfigurationCommon"/>
    <bean class="sk.qbsw.security.organization.simple.management.db.autoconfigure.SPOSecurityManagementAutoConfiguration"/>
