<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0                               http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>sk.qbsw</groupId>
	<artifactId>coconut</artifactId>
	<version>1.10.1.1</version>
	<packaging>pom</packaging>
	<name>[CoconutTree]</name>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<!-- compiling projects -->
		<compiler.version>2.3.2</compiler.version>
		<java.version>1.6</java.version>
		<jasypt.version>1.9.0</jasypt.version>

		<!-- QBSW dependences -->
		<droid.version>${project.version}</droid.version>
		<indy.version>${project.version}</indy.version>
		<core.version>${project.version}</core.version>
		<winnetou.version>${project.version}</winnetou.version>

		<!-- ENTERPRISE -->
		<!-- Spring -->
		<spring.version>4.0.5.RELEASE</spring.version>
		<spring.security.version>3.2.4.RELEASE</spring.security.version>
		<spring.ws.version>2.1.4.RELEASE</spring.ws.version>
		<!-- Hibernate -->
		<hibernate.version>4.2.13.Final</hibernate.version>
		<hibernate-entitymanager.version>4.2.13.Final</hibernate-entitymanager.version>
		<javaee.version>6.0</javaee.version>
		<apache-velocity.version>1.7</apache-velocity.version>
		<jpa.version>2.0-cr-1</jpa.version>
		<jta.version>1.1</jta.version>
		<persistence-api.version>1.0</persistence-api.version>
		<dbcp.version>1.3</dbcp.version>
		<dom4j.version>1.6.1</dom4j.version>
		<javax-mail.version>1.4.1</javax-mail.version>
		<jetty.version>6.1.25</jetty.version>
		<postgresql-driver.version>9.0-801.jdbc4</postgresql-driver.version>
		<mssql-driver.version>4.0</mssql-driver.version>

		<!-- UTILS -->
		<log4j.version>1.2.16</log4j.version>
		<slf4j.version>1.5.8</slf4j.version>
		<mockito.version>1.9.5</mockito.version>
		<xstream.version>1.2.2</xstream.version>

		<antlr.version>2.7.7</antlr.version>
		<aspectj.version>1.7.1</aspectj.version>

		<velocity.version>1.7</velocity.version>
		<mail.version>1.4.3</mail.version>

		<commons-beanutils.version>1.8.3</commons-beanutils.version>
		<commons-collections.version>3.2.1</commons-collections.version>
		<commons-io.version>2.4</commons-io.version>
		<commons-lang.version>2.6</commons-lang.version>
		<commons-fileupload>1.2.2</commons-fileupload>

		<junit.version>4.10</junit.version>

		<!-- GSON DEPENDENCY -->
		<gson.version>2.0</gson.version>
		<jackson.version>1.9.9</jackson.version>
		<httpclient.version>4.2.1</httpclient.version>

		<!-- Wicket -->
		<wicketstuff.minis>1.5-RC5.1</wicketstuff.minis>
		<wicket.version>6.6.0</wicket.version>
		<wicket-spring-annot.version>1.3.7</wicket-spring-annot.version>

		<!-- SMARTGWT, GWT= -->
		<gwt.version>2.0.3</gwt.version>
		<gwtrpcspring.version>1.01</gwtrpcspring.version>
		<gwtmavenplugin.version>1.2</gwtmavenplugin.version>
		<smartgwt.version>2.1</smartgwt.version>

		<!-- joda time -->
		<joda-time.version>2.1</joda-time.version>
		<jadira.usertype.version>2.0.1</jadira.usertype.version>

		<!-- Apache directory API - LDAP -->
		<directory.api.version>1.0.0-M20</directory.api.version>
		<!-- ANDROID DEPENDENCIES -->
		<android-platform.version>2.2.1</android-platform.version>
		<android.platform>8</android.platform>
		<android-support.version>r7</android-support.version>
		<android-maps.version>14.0.0</android-maps.version>
		<java.version>1.6</java.version>
		<core-api.version>1.6.0</core-api.version>
		<!-- maven plugins -->
		<android-maven-plugin.version>3.1.1</android-maven-plugin.version>
		<maven-jarsigner-plugin.version>1.2</maven-jarsigner-plugin.version>
		<maven-resources-plugin.version>2.5</maven-resources-plugin.version>
		<maven-clean-plugin.version>2.5</maven-clean-plugin.version>
		<!-- orm lite -->
		<ormlite-core.version> 4.41 </ormlite-core.version>
		<ormlite-android.version> 4.41 </ormlite-android.version>
		<!-- gson -->
		<gson.version> 2.2.2 </gson.version>
		<httpcomponents.version> 4.2.1 </httpcomponents.version>
		<google-api-client.version> 1.0.6-alpha </google-api-client.version>
		<!-- ACRA -->
		<acra.version> 4.3.0 </acra.version>
		<!-- google play services -->
		<google-play-services.version>4.0.0</google-play-services.version>
		<!-- quartz -->
		<quartz.version>1.8.6</quartz.version>
	</properties>

	<modules>
		<module>droid</module>
		<module>core</module>
		<module>indy</module>
		<module>winnetou</module>
	</modules>
	<repositories>
		<repository>
			<id>jboss-public-repository-group</id>
			<name>JBoss Public Maven Repository Group</name>
			<url>https://repository.jboss.org/nexus/content/groups/public-jboss/</url>
			<layout>default</layout>
			<releases>
				<enabled>true</enabled>
				<updatePolicy>never</updatePolicy>
			</releases>
			<snapshots>
				<enabled>true</enabled>
				<updatePolicy>never</updatePolicy>
			</snapshots>
		</repository>
		<repository>
			<id>wicketstuff</id>
			<url>http://wicketstuff.org/maven/repository</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
			<releases>
				<enabled>true</enabled>
			</releases>
		</repository>
	</repositories>

	<distributionManagement>
		<repository>
			<id>artifactory.qbsw.local</id>
			<name>hades.qbsw.local-releases</name>
			<url>https://artifactory.qbsw.local/artifactory/qbsw-releases-local</url>
		</repository>
		<snapshotRepository>
			<id>artifactory.qbsw.local</id>
			<name>hades.qbsw.local-snapshots</name>
			<url>https://artifactory.qbsw.local/artifactory/qbsw-releases-local</url>
		</snapshotRepository>
	</distributionManagement>

</project>