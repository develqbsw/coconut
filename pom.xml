<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0                               http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>sk.qbsw</groupId>
	<artifactId>coconut</artifactId>
	<version>1.3.0</version>
	<packaging>pom</packaging>
	<name>[CoconutTree]</name>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<!-- compiling projects -->
		<compiler.version>2.3.2</compiler.version>
		<java.version>1.6</java.version>
		<jasypt.version>1.9.0</jasypt.version>

		<!-- QBSW dependences -->
		<indy.version>1.2.0</indy.version>
		<core.version>1.2.0</core.version>
		
		<!-- validated -->
		<spring.version>3.0.5.RELEASE</spring.version>
		<hibernate.version>3.6.7.Final</hibernate.version>
		<hibernate-entitymanager.version>3.6.7.Final</hibernate-entitymanager.version>
		<log4j.version>1.2.16</log4j.version>
		<wicket.version>6.6.0</wicket.version>
		<wicket-spring-annot.version>1.3.7</wicket-spring-annot.version>
		<antlr.version>2.7.7</antlr.version>
		<javaee.version>6.0</javaee.version>
		<aspectj.version>1.7.1</aspectj.version>

		<apache-velocity.version>1.7</apache-velocity.version>
		<commons-beanutils.version>1.8.3</commons-beanutils.version>
		<commons-collections.version>3.2.1</commons-collections.version>
		<commons-io.version>2.4</commons-io.version>
		<commons-lang.version>2.6</commons-lang.version>
		<commons-fileupload>1.2.2</commons-fileupload>
		<jpa.version>2.0-cr-1</jpa.version>
		<jta.version>1.1</jta.version>
		<persistence-api.version>1.0</persistence-api.version>
		<slf4j.version>1.5.8</slf4j.version>

		
		<!-- Other dependences -->
		<dbcp.version>1.3</dbcp.version>
		<dom4j.version>1.6.1</dom4j.version>
		<javax-mail.version>1.4.1</javax-mail.version>
		<jetty.version>6.1.25</jetty.version>
		<junit.version>4.10</junit.version>
		<postgresql-driver.version>9.0-801.jdbc4</postgresql-driver.version>
		
		<!-- GSON DEPENDENCY -->
		<gson.version>2.0</gson.version>
		<jackson.version>1.9.9</jackson.version>
		<httpclient.version>4.2.1</httpclient.version>
		
		<!-- Wicketstuff -->
		<wicketstuff.minis>1.5-RC5.1</wicketstuff.minis>
		
	</properties>

	<modules>
		<module>core</module>
		<module>indy</module>
	</modules>
	<repositories>
		<repository>
			<id>jboss-public-repository-group</id>
			<name>JBoss Public Maven Repository Group</name>
			<url>https://repository.jboss.org/nexus/content/groups/public-jboss/</url>
			<layout>default</layout>
			<releases>
				<enabled>true</enabled>
				<updatePolicy>never</updatePolicy>
			</releases>
			<snapshots>
				<enabled>true</enabled>
				<updatePolicy>never</updatePolicy>
			</snapshots>
		</repository>
		<repository>
			<id>wicketstuff</id>
			<url>http://wicketstuff.org/maven/repository</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
			<releases>
				<enabled>true</enabled>
			</releases>
		</repository>
	</repositories>
</project>